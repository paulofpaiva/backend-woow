# Database (match docker-compose credentials)
DATABASE_URL=postgresql://woow:woow_secret@localhost:5432/woow_db

# Server
PORT=3000

# JWT (use a long random string in production)
JWT_SECRET=your_jwt_secret_change_in_production
JWT_EXPIRES_IN=7d

# Cookie (optional: name for httpOnly cookie)
AUTH_COOKIE_NAME=access_token
AUTH_COOKIE_MAX_AGE_DAYS=7
